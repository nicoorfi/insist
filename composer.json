{
    "name": "nicoorfi/insist",
    "type": "library",
    "description": "Method wrappers around eventsauce/backoff, that retries callables actions thay may fail.",
    "keywords": [
        "nicoorfi",
        "insistjj"
    ],
    "homepage": "https://github.com/nicoorfi/insist",
    "license": "MIT",
    "authors": [
        {
            "name": "Nico Orfanos",
            "email": "nico@sigmie.com",
            "homepage": "https://github.com/nicoorfi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "eventsauce/backoff": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nicoorfi\\Insist\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nicoorfi\\Insist\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "version": "1.0.0"
}
